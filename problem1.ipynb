   "source": [
    "# requirements\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import cvxpy as cv"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 311,
   "id": "9838aac1",
   "metadata": {},
   "outputs": [],
   "source": [
    "# dataset 4\n",
    "n = 200\n",
    "x_d4 = 3 * (np.random.rand(n, 4) - 0.5)\n",
    "y_d4 = (2 * x_d4[:, 0] - 1 * x_d4[:,1] + 0.5 + 0.5 * np.random.randn(n)) > 0\n",
    "y_d4 = 2 * y_d4 -1"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "2f0a2d63",
   "metadata": {},
   "source": [
    "# Problemï¼‘"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 312,
   "id": "c9fcfe4a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Prepare\n",
    "X = np.column_stack([np.ones(n), x_d4])\n",
    "y = y_d4"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 313,
   "id": "572eb221",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Logistic regression functions\n",
    "def sigmoid(z):\n",
    "    return np.where(z >= 0, 1 / (1 + np.exp(-z)), np.exp(z) / (1 + np.exp(z)))\n",
    "\n",
    "def logistic_loss(w, X, y):\n",
    "    z = y * (X @ w)\n",
    "    return np.sum(np.log(1 + np.exp(-z)))\n",
    "\n",
    "def logistic_gradient(w, X, y):\n",
    "    z = y * (X @ w)\n",
    "    sigmoid_z = sigmoid(-z)\n",
    "    return -X.T @ (y * sigmoid_z)\n",
    "\n",
    "def logistic_hessian(w, X, y):\n",
    "    z = y * (X @ w)\n",
    "    sigmoid_z = sigmoid(-z)\n",
    "    weights = sigmoid_z * (1 - sigmoid_z)\n",
    "    return X.T @ (weights[:, np.newaxis] * X)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 314,
   "id": "4205db9a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Lipschitz constant calculation\n",
    "def compute_lipschitz_constant(X):\n",
    "    XTX = X.T @ X\n",
    "    eigenvalues = np.linalg.eigvals(XTX)\n",
    "    L = 0.25 * np.max(eigenvalues)\n",
    "    return L\n",
    "\n",
    "L = compute_lipschitz_constant(X)\n",
    "optimal_lr = 1.0 / L  # Optimal learning rate"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 315,
   "id": "d2ec9a0c",
   "metadata": {},
   "outputs": [],
   "source": [
    "loss_hist_batch = [] # to store the history of loss J(w_t)\n",
    "\n",
    "# implement batch steepest gradient method here\n",
    "def batch_gradient_descent(X, y, max_iter=100, lr=0.1, tol=1e-8):\n",
    "    w = np.zeros(X.shape[1])  # Initialize weights\n",
    "    loss_history = []\n",
    "\n",
    "    for t in range(max_iter):\n",
    "        # Compute loss\n",
    "        loss = logistic_loss(w, X, y)\n",
    "        loss_history.append(loss)\n",
    "\n",
    "        # Compute gradient\n",
    "        grad = logistic_gradient(w, X, y)\n",
    "\n",
    "        # Check convergence\n",
    "        if np.linalg.norm(grad) < tol:\n",
    "            print(f\"Batch GD converged at iteration {t}\")\n",
    "            break\n",
    "\n",
    "        # Update weights\n",
    "        w -= lr * grad\n",
    "\n",
    "    return w, loss_history\n",
    "\n",
    "w_batch, loss_hist_batch = batch_gradient_descent(X, y, max_iter=100, lr=optimal_lr)"
   ]
  },
